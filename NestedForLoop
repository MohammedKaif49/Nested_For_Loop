//# Nested_For_Loop
//Write a C program to print the following pattern 01 0101 010101 01010101 0101010101 010101010101
#include<stdio.h>
int main(){ 
	int i,j, n;
	printf("Enter no. of rows :");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		for(j=0;j<i;j++){
			printf("01");
		}
		printf("\n");
	}
	return 0; 
}
